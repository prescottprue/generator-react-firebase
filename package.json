{
  "name": "generator-react-firebase",
  "version": "2.0.0-beta.4",
  "description": "Starter that uses React and Firebase (with option for Redux)",
  "main": "generators/index.js",
  "scripts": {
    "lint": "eslint generators tests",
    "lint:fix": "npm run lint -- --fix",
    "test": "mocha -R spec --compilers js:babel-core/register ./tests/setup.js ./tests/**/*.spec.js && npm run test:clean",
    "test:cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- ./tests/setup.js ./tests/**/*.spec.js --recursive --report lcov --compilers js:babel-register && npm run test:clean",
    "test:clean": "rimraf tests/tmp",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "prepare": "npm run lint",
    "prepush": "npm run lint"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "command-exists": "^1.2.2",
    "yeoman-generator": "^1.1.1",
    "yosay": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.0",
    "codecov": "^2.2.0",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^4.0.1",
    "husky": "^0.14.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.7.0"
  },
  "homepage": "github.com/prescottprue/generator-react-firebase",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/generator-react-firebase.git"
  },
  "author": {
    "name": "Scott Prue",
    "email": "sprue.dev@gmail.com",
    "url": "https://github.com/prescottprue"
  },
  "keywords": [
    "react",
