{
  "name": "redux-firestore",
  "version": "0.0.1",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:dist": "npm run build && firebase serve --only hosting -p 3000",
    "test": "react-scripts test",
    "build:config": "firebase-ci createConfig",
    "build:testConfig": "cypress-firebase createTestEnvFile",
    "deploy": "firebase-ci deploy -s",
    "functions:start": "npm run start --prefix functions",
    "functions:serve": "npm run serve --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:test": "npm run test --prefix functions",
    "test:ui": "npm run build:testConfig && cypress run",
    "test:ui:open": "npm run build:testConfig && cypress open",
    "test:ui:stage": "npm run test:ui -- --env envName=stage",
    "test:ui:open:stage": "npm run test:ui:open -- --env envName=stage",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "pre-push": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/redux-firestore.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "author": "prescottprue (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "@sentry/browser": "^4.6.4",
    "firebase": "^5.8.6",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-ga": "^2.3.5",
    "react-google-button": "^0.5.3",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.2.6",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.3",
    "redux-firestore": "^0.6.0",
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "next",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "cypress": "^3.1.5",
    "cypress-firebase": "^0.1.4",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-plugin-babel": "^5.3.0",    
    "eslint-plugin-flowtype": "^2.0.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "firebase-ci": "^0.5.6",
    "firebase-tools": "^6.4.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "node-sass": "^4.10.0",
    "prettier": "^1.16.4",
    "react-scripts": "2.1.5",
    "react-test-renderer": "^16.8.3"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
