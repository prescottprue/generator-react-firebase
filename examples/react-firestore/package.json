{
  "name": "react-firestore",
  "version": "0.0.1",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "setup": "echo \"Generating client config...\" && env-cmd bin/generate-firebase-sdk-config.js --file ./config/default.json",
    "check-config": "if [ ! -f ./src/client-config.json ]; then yarn setup; else echo \"Client config already exists, running command...\"; fi;",
    "start": "yarn check-config && env-cmd craco start",
    "start:dist": "yarn build && firebase emulators:start --only hosting",
    "start:emulate": "cross-env NODE_APP_INSTANCE=emulators yarn start",
    "test": "craco test",
    "functions:start": "yarn --cwd functions start",
    "functions:watch": "yarn --cwd functions watch",
    "functions:build": "yarn --cwd functions build",
    "functions:test": "firebase emulators:exec --only auth,firestore,database,pubsub \"yarn --cwd functions test:base\"",
    "functions:test:cov": "firebase emulators:exec --only auth,firestore,database,pubsub \"yarn --cwd functions test:cov:base\"",
    "test:ui:run": "env-cmd cypress run",
    "test:ui": "env-cmd cypress open",
    "test:ui:emulate": "cross-env NODE_APP_INSTANCE=emulators npm run test:ui",
    "emulators": "firebase emulators:start --only auth,firestore,database,pubsub",
    "emulators:hosting": "firebase emulators:start --only auth,firestore,database,hosting",
    "emulators:all": "firebase emulators:start",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prescottprue/react-firestore.git"
  },
  "engines": {
    "node": ">14.15.0"
  },
  "author": "prescottprue (https://github.com/prescottprue)",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.3",
    "@mui/material": "^5.0.3",
    "@mui/styles": "^5.0.1",
    "@sentry/browser": "^6.12.0",
    "firebase": "^9.1.1",
    "notistack": "^2.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-button": "^0.7.2",
    "react-hook-form": "^7.15.2",
    "react-router-dom": "^5.3.0",
    "reactfire": "^4.2.0",
    "stackdriver-errors-js": "^0.8.0"
  },
  "devDependencies": {
    "@craco/craco": "6.2.0",
    "@sentry/cli": "^1.68.0",
    "@testing-library/cypress": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-eslint": "^10.1.0",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "cypress": "^8.3.1",
    "cypress-firebase": "^2.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "firebase-admin": "^9.11.0",
    "firebase-ci": "^0.15.0",
    "firebase-tools": "^9.16.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "minimist": "1.2.5",
    "prettier": "^2.3.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts}": [
      "eslint --fix"
    ]
  },
  "config": {
    "port": 3000
  }
}
