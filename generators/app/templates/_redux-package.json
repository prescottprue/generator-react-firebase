{
  "name": "<%= appName %>",
  "version": "<%= version %>",
  "main": "bin/server.js",
  "scripts": {
    "build": "npm run clean && npm run build-bundle && npm run build-server && npm run build-copy",
    "build-bundle": "webpack -p --stats --progress --config webpack-production.config",
    "build-server": "webpack -p --stats --progress --config webpack-server-production.config",
    "build-copy": "node ./bin/copy",
    "lint": "eslint -c .eslintrc ./app",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf build",
    "start": "node bin/dev-server",
    "production": "node bin/server"
  },
  "author": "<%= githubUser %>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/<%= githubUser %>/<%= appName %>"
  },
  "bugs": {
    "url": "https://github.com/<%= githubUser %>/<%= appName %>/issues"
  },
  "keywords": [
    "webpack",
    "redux",
    "react",
    "react-router"
  ],
  "dependencies": {
    "express": "4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "firebase": "^3.2.1",
    "history": "1.13.0",
    "immutable": "^3.8.1",
    "lodash": "^4.14.1",
    "material-ui": "0.15.2",
    "proxy-middleware": "^0.15.0",
    "react": "0.14.6",
    "react-dom": "0.14.6",
    "react-google-button": "^0.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.0.0",
    "react-router-redux": "4.0.0-rc.1",
    "react-tap-event-plugin": "0.2.1",
    "redux": "3.2.1",
    "redux-firebasev3": "^0.1.0",
    "redux-form": "^5.3.1",
    "redux-thunk": "^2.1.0",
    "serve-static": "1.10.2",
    "webpack": "1.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-plugin-lodash": "^3.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-loader": "^6.2.4",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.19.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.3.2",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.12.3",
    "transform-loader": "^0.2.3",
    "webpack-dev-server": "1.14.0"
  }
}
