{
  "name": "<%= appName %>",
  "version": "<%= version %>",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "node scripts/build.js",
    "build:functions": "npm run build --prefix functions",
    "deploy": "firebase-ci deploy -s",
    "createConfig": "firebase-ci createConfig",
    "start": "node scripts/start.js",<% if (includeTests) { %>
    "test": "node scripts/test.js",<% } %>
    "lint": "eslint src functions scripts config<% if (includeTests) { %> tests<% } %>",
    "lint:fix": "npm run lint -- --fix",<% if (includeFunctions) { %>
    "functions:start": "npm run start --prefix functions",
    "functions:serve": "npm run serve --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:test": "npm run test --prefix functions",<% } %>
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",<% if (deployTo !== 'firebase') { %>
    "create-config": "node build/create-config",<% } %><% if (includeTests) { %>
    "codecov": "cat coverage/*/lcov.info | codecov",<% } %>
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUser %>/<%= appName %>.git"
  },
  "engines": {
    "node": ">=8"
  },
  "author": "<%= githubUser %> (https://github.com/<%= githubUser %>)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "firebase": "^5.5.8",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.1",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",<% if (includeAnalytics) { %>
    "react-ga": "^2.3.5",<% } %>
    "react-google-button": "^0.5.3",
    "react-loadable": "^5.5.0",
    <% if (includeRedux) { %>"react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.9",<% } %>
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",<% if (includeRedux) { %>
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^1.0.0",<% } %><% if (includeRedux && includeFirestore) { %>
    "redux-firestore": "^0.5.8",<% } %><% if (includeRedux) { %>
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "next",
    "redux-thunk": "^2.2.0"<% } %>
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@svgr/webpack": "2.4.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.4",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",<% if (includeTests) { %>
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^1.0.0-beta.0",<% } %>
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "prettier": "^1.14.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.5",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",<% if (includeTests) { %>
    "sinon": "^5.1.1",
    "sinon-chai": "^3.1.0",<%} %>
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
}
