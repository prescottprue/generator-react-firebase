{
  "name": "<%= appName %>",
  "version": "<%= version %>",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "build:prod": "echo \"build:prod is deprecated. Use npm run build.\" && npm run build",
    "build:functions": "npm run build --prefix functions",
    "deploy": "firebase-ci deploy -s",
    "createConfig": "firebase-ci createConfig",
    "start": "cross-env NODE_ENV=development webpack-dashboard -p 1234 -c cyan -m -t <%= appName %> -- node build/scripts/start",
    "start:simple": "cross-env NODE_ENV=development node build/scripts/start",<% if (includeTests) { %>
    "test": "cross-env NODE_ENV=test karma start build/karma.config",
    "test:watch": "npm test -- --watch",<% } %>
    "lint": "eslint src functions server build project.config.js<% if (includeTests) { %> tests<% } %>",
    "lint:fix": "npm run lint -- --fix",<% if (includeFunctions) { %>
    "functions:start": "npm run start --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "functions:test": "npm run test --prefix functions",<% } %>
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",<% if (deployTo !== 'firebase') { %>
    "create-config": "node build/create-config",<% } %><% if (includeTests) { %>
    "codecov": "cat coverage/*/lcov.info | codecov",<% } %>
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUser %>/<%= appName %>.git"
  },
  "engines": {
    "node": ">=6.14.0"
  },
  "author": "<%= githubUser %> (https://github.com/<%= githubUser %>)",
  "license": "MIT",
  "dependencies": {<% if (materialv1) { %>
    "@material-ui/core": "^1.2.1",
    "@material-ui/icons": "^1.1.0",<% } %>
    "firebase": "^5.0.4",
    "lodash": "^4.17.10",<% if (!materialv1) { %>
    "material-ui": "^0.20.0",<% } %>
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "promise": "^7.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",<% if (includeAnalytics) { %>
    "react-ga": "^2.3.5",<% } %>
    "react-google-button": "^0.3.0",
    <% if (includeRedux) { %>"react-redux": "^5.0.7",
    "react-redux-firebase": "^2.1.3",<% } %>
    "react-router": "^3.0.0",<% if (!materialv1) { %>
    "react-tap-event-plugin": "^2.0.1",<% } %>
    "recompose": "^0.27.1",
    "redbox-react": "^1.3.6",<% if (includeRedux) { %>
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^1.0.0",<% if (includeRedux && includeFirestore) { %>
    "redux-firestore": "^0.5.6",<% } %>
    "redux-form": "^7.4.0",
    <% if (materialv1) { %>"redux-form-material-ui": "next",<% } %><% if (!materialv1) { %>"redux-form-material-ui": "^4.2.0",<% } %>
    "redux-thunk": "^2.2.0",<% } %><% if (includeErrorHandling) { %>
    "stackdriver-errors-js": "^0.4.0",<% } %>
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.20.0",<% if (includeTests) { %>
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^1.0.0-beta.0",<% } %>
    "chalk": "^1.1.3",<% if (includeTests) { %>
    "cheerio": "0.22.0",
    "codecov": "^3.0.2",<% } %>
    "compression": "^1.6.2",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",<% if (includeTests) { %>
    "dirty-chai": "^1.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",<% } %>
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "figures": "^2.0.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "ip": "^1.1.5",<% if (includeTests) { %>
    "istanbul-instrumenter-loader": "^3.0.0-rc.1",
    "karma": "^1.7.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "mocha": "^3.2.0",<% } %>
    "node-sass": "^4.5.3",<% if (includeTests) { %>
    "phantomjs-polyfill": "^0.0.2",
    "phantomjs-prebuilt": "^2.1.14",<% } %>
    "prettier": "^1.12.1",<% if (includeTests) { %>
    "react-test-renderer": "^16.4.0",<% } %>
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",<% if (includeTests) { %>
    "sinon": "^5.1.1",
    "sinon-chai": "^3.1.0",<% } %>
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.6.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.13.2",
    "yargs": "^8.0.1"
  }
}
