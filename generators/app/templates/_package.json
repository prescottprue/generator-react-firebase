{
  "name": "<%= appName %>",
  "version": "<%= version %>",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start": "react-scripts start",
    "start:dist": "npm run build && firebase serve --only hosting -p 3000",<% if (includeComponentTests) { %>
    "test": "react-scripts test",<% } %><% if (deployTo !== 'firebase') { %>
    "create-config": "node build/create-config",<% } else { %>
    "build:config": "firebase-ci createConfig",<% } %><% if (includeUiTests) { %>
    "build:testConfig": "cypress-firebase createTestEnvFile",<% } %><% if (deployTo === 'firebase') { %>
    "deploy": "firebase-ci deploy -s",<% } %><% if (includeFunctions) { %>
    "functions:start": "npm run start --prefix functions",
    "functions:serve": "npm run serve --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:build": "npm run build --prefix functions",<% } %><% if (includeFunctionsTests) { %>
    "functions:test": "npm run test --prefix functions",<% } %><% if (includeUiTests) { %>
    "test:ui": "npm run build:testConfig && cypress run",
    "test:ui:open": "npm run build:testConfig && cypress open",
    "test:ui:stage": "npm run test:ui -- --env envName=stage",
    "test:ui:open:stage": "npm run test:ui:open -- --env envName=stage",<% } %>
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "pre-push": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<%= githubUser %>/<%= appName %>.git"
  },
  "engines": {
    "node": ">=8.11.0"
  },
  "author": "<%= githubUser %> (https://github.com/<%= githubUser %>)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",<% if (includeSentry) { %>
    "@sentry/browser": "^5.9.1",<% } %>
    "firebase": "^7.6.0",
    "formik": "^2.0.6",
    "formik-material-ui": "^1.0.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-google-button": "^0.6.0",
    "react-loadable": "^5.5.0",<% if (includeRedux) { %>
    "react-redux": "^7.1.1",
    "react-redux-firebase": "^3.0.5",<% } %>
    "react-router-dom": "^5.1.2",<% if (!includeRedux) { %>
    "reactfire": "^2.0.0-alpha.2",<% } %>
    "recompose": "^0.30.0"<% if (includeRedux) { %>,
    "redux": "^4.0.4",
    "redux-auth-wrapper": "^2.1.0",<% } %><% if (includeRedux && includeFirestore) { %>
    "redux-firestore": "^0.11.0",<% } %><% if (includeRedux) { %>
    "redux-thunk": "^2.3.0"<% } %>
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",<% if (includeUiTests) { %>
    "cypress": "^3.8.0",
    "cypress-firebase": "0.5.0",<% } %>
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",<% if (includeUiTests) { %>
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-cypress": "^2.7.0",<% } %>
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^18.4.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",<% if (deployTo === 'firebase') { %>
    "firebase-ci": "^0.10.0",
    "firebase-tools": "^7.11.0",<% } %>
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "react-scripts": "3.3.0"<% if (includeComponentTests) { %>,
    "react-test-renderer": "^16.12.0"<%} %>
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
